{
  "expo": {
    "name": "search-sort-redux",
    "slug": "search-sort-redux",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "A React-Native expo app that uses redux for state handling and redux-thunk for synchronization. Users can search and sort contacts along with pagination.",
    "githubUrl": "https://github.com/israr-shaikh/search-sort-redux-expo"
  }
}
